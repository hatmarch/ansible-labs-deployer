    app: homeroom
    role: lab
  name: homeroom-cfg-{{ internal_lab_workshop_app_name }}
data:
  workshops.json: | -
    [
      {
        "title": "{{ internal_lab_workshop_description }}",
        "description": "{{ internal_lab_workshop_description }}",
        "url": "{{ __lab_url }}"
      }
    ]